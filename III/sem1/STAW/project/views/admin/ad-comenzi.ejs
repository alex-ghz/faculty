<div class="clienti-section">
    <div class="produse">
        <% if ( comenzi.length === 0) { %>
            <h2>Nu exista momentan comenzi</h2>
        <% } else { %>
            <div class="table-wrapper">
                <div class="table">
                    <div class="table-head">
                        <table>
                            <thead>
                            <tr>
                                <th class="column-uno">Nume</th>
                                <th class="column-uno">Email</th>
                                <th class="column-dos">Oras</th>
                                <th class="column-dos">Telefon</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                    <div class="table-body">
                        <table>
                            <tbody>
                            <% comenzi && comenzi.forEach(client => { %>
                                <tr>
                                    <td class="column-uno"><%= client.firstname %></td>
                                    <td class="column-uno"><%= client.email %></td>
                                    <td class="column-dos"><%= client.oras %></td>
                                    <td class="column-dos"><%= client.telefon %></td>
                                </tr>
                            <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        <% } %>
    </div>
</div>